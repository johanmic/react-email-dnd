{
  "name": "@react-email-dnd/renderer",
  "author": "Johan Mickelin",
  "license": "MIT",
  "version": "0.3.1",
  "description": "Rendering utilities for react-email-dnd documents.",
  "keywords": [
    "react",
    "email",
    "dnd",
    "renderer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanmic/react-email-dnd.git",
    "directory": "packages/renderer"
  },
  "homepage": "https://dnd.email/docs/packages/renderer/rendering-examples",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@react-email-dnd/shared": "^0.3.0"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "react-email": "^3.0.0 || ^5.0.0",
    "@react-email/components": "^0.5.0"
  },
  "devDependencies": {
    "@react-email/components": "^0.0.22",
    "@types/react": "^19.1.16",
    "react-email": "^3.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}