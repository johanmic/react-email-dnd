{
  "name": "@react-email-dnd/shared",
  "author": "Johan Mickelin",
  "version": "0.2.0",
  "description": "Shared schema types and validation utilities for react-email-dnd.",
  "keywords": [
    "react",
    "email",
    "dnd",
    "shared",
    "schema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanmic/react-email-dnd.git",
    "directory": "packages/shared"
  },
  "homepage": "https://dnd.email/docs/json-structure",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest --config ./jest.config.ts"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@phosphor-icons/react": "^2.1.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.16",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}